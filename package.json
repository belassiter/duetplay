{
  "name": "temp_app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node scripts/generate-manifest.js --input public_duet --output public_duet/songs.json && node scripts/copy-assets.cjs public_duet/songs.json public/songs.json && vite",
    "dev:quartet": "node scripts/generate-manifest.js --input ../esquartet/sheetmusic --output public/quartet-songs.json --optional && vite --mode quartet",
    "build": "node scripts/generate-manifest.js --input public_duet --output public_duet/songs.json && tsc -b && vite build && node scripts/copy-assets.cjs public_duet dist",
    "build:quartet": "node scripts/generate-manifest.js --input ../esquartet/sheetmusic --output public/quartet-songs.json --optional && tsc -b && vite build --mode quartet && node scripts/copy-assets.cjs ../esquartet/sheetmusic dist-quartet --optional",
    "gen-manifest": "node scripts/generate-manifest.js",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "preview": "vite preview",
    "preview:quartet": "vite preview --outDir dist-quartet"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "jspdf": "^4.1.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.563.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "svg2pdf.js": "^2.7.0",
    "tailwind-merge": "^3.4.0",
    "tonal": "^6.4.3",
    "verovio": "^6.0.1",
    "vexflow": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.24",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.18"
  }
}
